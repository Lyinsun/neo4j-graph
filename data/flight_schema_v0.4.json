  "business_rules": {
    "state_machine": {
      "Flight": {
        "states": ["Created", "Planned", "Active", "Landed", "Canceled", "Closed"],
        "transitions": [
          {
            "from": "Planned",
            "to": "Active",
            "event": "FlightDeparture",
            "constraint": "必须分配跑道和飞机"
          },
          {
            "from": "Active",
            "to": "Landed",
            "event": "FlightArrival",
            "constraint": "必须记录实际到达时间"
          }
        ]
      },
      "Event": {
        "states": ["New", "Processing", "Closed", "Archived"],
        "transitions": [
          {
            "from": "New",
            "to": "Processing",
            "description": "系统开始执行动作链路"
          },
          {
            "from": "Processing",
            "to": "Closed",
            "description": "动作链路已全部执行完毕"
          }
        ]
      }
    },

    "constraints": {
      "Gate冲突约束": "同一时间段内，Gate不得被两个航班同时占用",
      "Turnaround约束": "后续航班起飞时间 >= 前序航班落地时间 + 最小过站时间",
      "Runway占用约束": "连续两架飞机使用跑道之间必须满足最小安全间隔",
      "Aircraft排班约束": "同一时刻一架飞机只能执行一个航班",
      "状态机约束": "航班状态必须按定义的生命周期路径迁移"
    },

    "event_action_mapping": {
      "FlightDelayedEvent": [
        "NotifyTowerAction - 通知塔台",
        "CheckGateConflictAction - 检查Gate冲突",
        "ReassignGateAction - 重新分配Gate（如有冲突）",
        "UpdateFlightStatusAction - 更新航班状态为Delayed"
      ],
      "FlightPlanChangedEvent": [
        "UpdateFlightPlanAction - 更新计划对象",
        "ReassignResourceAction - 重新分配资源"
      ],
      "ResourceConflictEvent": [
        "AlertConflictAction - 发送冲突告警",
        "ResolveConflictAction - 启动冲突解决流程"
      ]
    }
  },

  "query_examples": {
    "find_delayed_flights": {
      "description": "查找所有延误的航班",
      "cypher": "MATCH (f:Flight) WHERE f.status = 'Delayed' AND f.version = 'Flight0.4' RETURN f"
    },
    "find_flight_events": {
      "description": "查找某航班的所有事件",
      "cypher": "MATCH (e:Event)-[:IMPACTS]->(f:Flight {flightId: 'CA123-20240208'}) WHERE e.version = 'Flight0.4' RETURN e ORDER BY e.eventTime"
    },
    "find_gate_usage": {
      "description": "查找登机口的使用情况",
      "cypher": "MATCH (f:Flight)-[r:USES]->(g:Gate) WHERE g.version = 'Flight0.4' RETURN f.flightNumber, g.gateId, r.startTime, r.endTime"
    },
    "find_airport_resources": {
      "description": "查找机场管理的所有资源",
      "cypher": "MATCH (a:Airport {airportCode: 'ZBAA'})-[:MANAGES]->(r) WHERE a.version = 'Flight0.4' RETURN labels(r) AS resourceType, r"
    },
    "event_action_chain": {
      "description": "追踪事件触发的动作链",
      "cypher": "MATCH (e:Event {eventId: 'EVT-CA123-001'})-[t:TRIGGERS]->(f:Flight) WHERE e.version = 'Flight0.4' RETURN e.eventType, t.actionType, t.triggerTime, f.flightId ORDER BY t.triggerTime"
    }
  }
}
