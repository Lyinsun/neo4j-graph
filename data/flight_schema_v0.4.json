        "type": "MANAGES",
        "from": {"label": "Airport", "airportCode": "ZBAA"},
        "to": {"label": "Gate", "gateId": "T3-52"},
        "properties": {
          "version": "Flight0.4"
        }
      },
      {
        "type": "MANAGES",
        "from": {"label": "Airport", "airportCode": "ZBAA"},
        "to": {"label": "Gate", "gateId": "T3-66"},
        "properties": {
          "version": "Flight0.4"
        }
      },
      {
        "type": "MANAGES",
        "from": {"label": "Airport", "airportCode": "ZBAA"},
        "to": {"label": "Runway", "runwayId": "01L"},
        "properties": {
          "version": "Flight0.4"
        }
      },
      {
        "type": "MANAGES",
        "from": {"label": "Airport", "airportCode": "ZBAA"},
        "to": {"label": "ParkingStand", "standId": "245"},
        "properties": {
          "version": "Flight0.4"
        }
      },

      {
        "type": "CONTROLS",
        "from": {"label": "Tower", "towerId": "ZBAA-T3"},
        "to": {"label": "Runway", "runwayId": "01L"},
        "properties": {
          "version": "Flight0.4"
        }
      },

      {
        "type": "DERIVED_FROM",
        "from": {"label": "Flight", "flightId": "CA123-20240208"},
        "to": {"label": "FlightPlan", "planId": "FP-CA123-20240208"},
        "properties": {
          "version": "Flight0.4"
        }
      },

      {
        "type": "ASSIGNED_TO",
        "from": {"label": "Aircraft", "aircraftId": "B-1234"},
        "to": {"label": "Flight", "flightId": "CA123-20240208"},
        "properties": {
